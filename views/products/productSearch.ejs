
<%- include('../partials/head') %>
<link rel='stylesheet' href='/stylesheets/social.css' />
<link rel='stylesheet' href='/stylesheets/header.css' />
<link rel='stylesheet' href='/stylesheets/listing.css' />
<link rel='stylesheet' href='/stylesheets/footer.css' />
<link rel='stylesheet' href='/stylesheets/common.css' />
</head>

<body>
  
<%- include('../partials/social') %> 
<%- include('../partials/header') %> 


<section class="section">


<h1>SEARCH RESULTS</h1>

<hr>


<div class="d-flex f-row f-wrap j-arround">

    <% results.forEach(function(results) { %>

      <article class="hero w-30">
        <a href="/products/<%=results.id %>">
        <div class="hero-img">
          <img src= <%= '/images/' + results.detalle +'.png' %>   alt="" width="100%">
        </div>
        <div class="hero-name p-2">
          <h1><%=  results.detalle.toUpperCase().replace('-', " ") %></h1>
        </a>
      </article>

    <% })  %> 
  </div>

</section>

<%- include('../partials/backButton') %> 
  
  <%- include('../partials/footer') %> 
      
    </body>
  </html> 